<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}"</title>
  <link rel="profile" href="https://gmpg.org/xfn/11">
  <meta name="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
  <link rel="canonical" href="{{ site.url }}{{ page.url }}">
  {# Google Schema #}
  <meta itemprop="name" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta itemprop="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">

  {# Open Graph #}
  <meta property="og:locale" content="id_ID" >
  <meta property="og:type" content="website">
  <meta property="og:title" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta property="og:description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
  <meta property="og:url" content="{{ site.url }}{{ page.url }}">
  <meta property="og:site_name" content="{{ site.name }}">
  <meta property="og:updated_time" content="{{ modified_date }}">
  <meta property="og:image" content="{{ site.url }}{% if article_image %}{{ article_image }}{% else %}{{ site.logo }}{% endif %}">
  <meta property="og:image:secure_url" content="{{ site.url }}{% if article_image %}{{ article_image }}{% else %}{{ site.logo }}{% endif %}">
  <meta property="article:section" content="{% if navtitle %}{{ navtitle }}{% else %}{{ section }}{% endif %}">
  <meta property="article:published_time" content="{{ date }}">
  <meta property="article:modified_time" content="{{ modified_date }}">
  {% if tags %}
      {% for tag in tags %}
        {%- if tag != "nav" -%}
            <meta property="article:tag" content="{{ tag }}">
        {%- endif -%}
      {% endfor %}
  {% endif %}

  {# Twitter #}
  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta name="twitter:description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
  <meta name="twitter:image" content="{% if article_image %}{{ article_image }}{% else %}{{ site.logo }}{% endif %}">
  <meta name="twitter:site" content="{{ site.name }}">
  <meta name="twitter:creator" content="{{ author.twitter.name }}">
  <meta name="twitter:image:src" content="{{ site.url }}{% if article_image %}{{ article_image }}{% else %}{{ site.logo }}{% endif %}">
  <meta name="google-site-verification" content="Y-O10v6PFB7UjsyVtcKVeswDKxFM9ZLc7GR0i2EmJlY" />
  <link rel="icon" type="image/png" href="{{ metadata.url }}/static/img/favicon.ico" >
  <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel='stylesheet' href="//cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
  <link rel='stylesheet' href="//cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">
  <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/jQuery.mmenu/7.3.3/jquery.mmenu.all.css">
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js" type="text/javascript"></script>  

  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>

  {% set js %}
    {% include "assets/js/inline.js" %} 
    {% include "assets/js/jssor.slider.min.js" %} 
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
  <meta name="google-site-verification" content="FD6MXulZMvF-XEhwUcubemcZeQH7YIqT6GsMLgP-Wu0" >
</head>
