<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ site.name | capitalize }} | {{ title or site.title }}</title>
  <link rel="profile" href="https://gmpg.org/xfn/11">
  <meta name="description" content="{{ description or site.description }}">
  <link rel="canonical" href="{{ site.url }}{{ page.url }}">

  {# Google Schema #}
  <meta itemprop="name" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta itemprop="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">

  {# Open Graph #}
  <meta name="og:title" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta name="og:description" content="{% if meta_description %}{{ meta_description }}{% else %}{{ excerpt }}{% endif %}">
  <meta name="og:image" content="{{ site.url }}{{ site.images.og }}">
  <meta name="og:url" content="{{ site.url }}{{ page.url }}">
  <meta name="og:site_name" content="{{ site.name }}">


  <meta property="og:locale" content="id_ID">
  <meta property="og:type" content="website">
  <meta property="og:title" content="{{ title or site.title }}">
  <meta property="og:description" content="{{ description or site.description }}">
  <meta property="og:url" content="{{ permalink or site.url }}">
  <meta property="og:site_name" content="{{ site.name }}">
  <meta property="og:updated_time" content="{{ modified_date }}">
  <meta property="og:image" content="{{ article_image }}">
  <meta property="og:image:secure_url" content="{{ article_image }}">
  {% if tags %}
      {% for tag in tags %}
        {%- if tag != "nav" -%}
            <meta property="article:tag" content="{{ tag }}">
        {%- endif -%}
      {% endfor %}
  {% endif %}
 
  <meta property="article:section" content="{{ navtitle }}">
  <meta property="article:published_time" content="{{ date }}">
  <meta property="article:modified_time" content="{{ modified_date }}">

  {# Twitter #}
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:description" content="{{ description or site.description }}">
  <meta name="twitter:title" content="{{ title or site.title }}">
  <meta name="twitter:image" content="{{ article_image }}">
  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="{% if title %}{{ title }}{% else %}{{ site.title }}{% endif %} | {{ site.name }}">
  <meta name="twitter:description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
  <meta name="twitter:site" content="{{ site.name }}">
  <meta name="twitter:creator" content="{{ author.twitter.name }}">
  <meta name="twitter:image:src" content="{{ site.url }}{{ article_image }}">







  <link rel="icon" type="image/png" href="{{ metadata.url }}/static/img/favicon.ico" />
  <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel='stylesheet' href="//cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
  <link rel='stylesheet' href="//cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">
  <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/jQuery.mmenu/7.3.3/jquery.mmenu.all.css">
  <script src="//identity.netlify.com/v1/netlify-identity-widget.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js" type="text/javascript"></script>  

  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>

  {% set js %}
    {% include "assets/js/inline.js" %} 
    {% include "assets/js/jssor.slider.min.js" %} 
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
